  <!--     name="cameraFrameToCameraOptical" args="0 0 0 -1.5708 0 -1.5708 /$(camera)_frame /$(camera)_optical_frame 100" /> -->

  <param name="/$(camera)_optical_frame/hfov" value="" />
  <param name="/$(camera)_optical_frame/vfov" value="" />


  <!--<env name="ROSCONSOLE_CONFIG_FILE" -->
    <!--value="$(find pandora_alert_handler)/config/alert_handler/logger_debug.cfg"/>-->
  <include file="$(find frame_matcher)/launch/frame_matcher.launch" />
  <rosparam param="subscribed_topic_names/map" ns="/pandora_data_fusion/frame_matcher">/map</rosparam>

  <test test-name="FrameMatcherFunctionalTest" pkg="frame_matcher" type="frame_matcher_functional.py" />

</launch>
